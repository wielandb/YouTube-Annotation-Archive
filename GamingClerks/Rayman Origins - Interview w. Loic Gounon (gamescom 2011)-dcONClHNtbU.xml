  <TEXT>Gut so!</TEXT>
  <segment>
    <movingRegion type="rect">
      <rectRegion x="60.46900" y="69.16700" w="9.37500" h="7.22200" t="0:00:25.0"/>
      <rectRegion x="60.46900" y="69.16700" w="9.37500" h="7.22200" t="0:00:25.7"/>
